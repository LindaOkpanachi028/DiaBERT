{
    "manifest_version": 3,
    "name": "DiaBERT Classifier",
    "version": "1.0",
    "description": "Classify diabetes-related text and get explanations.",
    "action": {
      "default_popup": "popup.html",
      "default_title": "DiaBERT Misinformation Classifier"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://127.0.0.1:5000/*"
    ]
  }
  